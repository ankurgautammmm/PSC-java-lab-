class Electricity{
    int units;
    double bill;
    Electricity(int u){
        units =u;
    }
    void calculateBill (){
        bill = units * 5;
    }
    public static void main(String[]args){
        Electricity e = new Electricity(200);
        e.calculateBill();
        System.out.println("Units;"+e.units);
        System.out.println("Bill;"+e.bill);
    }
}
